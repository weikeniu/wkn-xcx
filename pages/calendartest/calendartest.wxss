.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.textStyle {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.textContainer {
  margin-top: 200px;
}
.calendar{
  background: #fff;
  font-family: microsoft yahei,simhei;
}
.thead{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  background: #f9f9f9;
  border-bottom: 1rpx solid #ddd;
}
.mth{
  text-align: center;
  font-size: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.dtd{
  overflow: hidden;
}
.td{
  width: 107.142rpx;
  height: 70rpx;
  line-height: 70rpx; 
  text-align: center;
  font-size: 24rpx;
}
.td.dis{
  color: #ccc;
}
.td.in{
  color: #fff;
  background: #12b7f5;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  line-height: 35rpx;
  
}
.td.leave{
  color: #fff;
  background: #12b7f5;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  line-height: 35rpx;
  
  
}