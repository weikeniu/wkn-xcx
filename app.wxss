/*1、正在加载中提示*/
.page-loading{color: #666; font-size: 26rpx; font-family: helvetica; padding: 20rpx 0; text-align: center;}
.page-loading .loading-text{display: inline-block; vertical-align: middle;}
.weui-loading {
  display: inline-block;vertical-align: middle; margin-right: 5rpx;
  height: 20px;width: 20px;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}


/*2、正在加载中提示(背景遮罩层)*/
.page-loading__mask{background: rgba(0,0,0,.5); height: 100%; width: 100%; position: fixed; top: 0; left: 0; z-index: 9999;}
.page-loading__mask .inner{background: rgba(0,0,0,.5); border-radius: 10rpx; margin:0 auto; margin-top: -75rpx; height: 150rpx; width: 150rpx; position: relative; top: 50%;}
.page-loading__mask .inner .img{margin: -50rpx 0 0 -50rpx; height: 100rpx; width: 100rpx; position: absolute; left: 50%; top: 50%;}
.page-loading__mask .inner .img image{display: block; height: 100rpx; width: 100rpx;}
.page-loading__mask .rollImg{animation: rollLoading 1500ms linear infinite;}
@keyframes rollLoading{
	from{transform: rotate(0);}
	to{transform: rotate(360deg);}
}
@-webkit-keyframes rollLoading{
	from{-webkit-transform: rotate(0);}
	to{-webkit-transform: rotate(360deg);}
}


